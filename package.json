{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "cross-env NODE_ENV=production npm run clean && npm run lint && tsc",
    "start": "node build/main.js",
    "start:dev": "ts-node --files src/main.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint --fix .",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:generate": "npm run typeorm -- -d src/config/data-source.ts migration:generate",
    "typeorm:migrate": "npm run typeorm -- -d src/config/data-source.ts migration:run",
    "typeorm:revert": "npm run typeorm -- -d src/config/data-source.ts migration:revert",
    "postinstall": "patch-package",
    "seed": "ts-node src/seeders",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "argon2": "^0.30.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pino": "^8.7.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "patch-package": "^6.5.0",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
